<h1>Log In to Your Account</h1>
<div>
  <%= form_tag sessions_path do %>
    <div>
      <%= label_tag :email %>
      <%= email_field_tag :email %>
    </div>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div>
      <%= submit_tag "Sign In" %>
    </div>
  <% end %>
</div>
